    "sass-loader": "^7.3.1"
  },
  "devDependencies": {
    "@kocal/semantic-release-preset": "^2.0.1",
    "http-server": "^0.12.1",
    "jest": "^25.3.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^2.1.1",
    "semantic-release": "^17.0.4",
    "start-server-and-test": "^1.10.11"
  }
